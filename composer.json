{
    "name": "php-jsonrpc/poc-monorepo",
    "description": "Organisation monorepo",
    "license": "proprietary",
    "type": "project",
    "authors": [
        {
            "name": "Yoanm",
            "email": "yoanm@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/filesystem": "^6.2",
        "symfony/process": "^6.2",
        "symfony/yaml": "^6.2",
        "symplify/monorepo-builder": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "PhpJsonrpc\\PocMonorepoPkg1\\": "packages/scope1/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpJsonRpc\\Builder\\": "builder/src",
            "Symplify\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "replace": {
        "php-jsonrpc/poc-monorepo-pkg1": "0.1-dev"
    },
    "minimum-stability": "stable"
}
